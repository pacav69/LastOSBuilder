[Main]
Title=SlideToShutDown
Type=XPEPlugin
Author=ChrisR
Description=Slide To Shutdown feature let you shutdown Win10XPE by sliding a touch screen down, or dragging a screen down
Provides a user interface to shutdown Win10XPE on a touch device
Credits=teik
Version=001
Download_Level=2
Level=5
Selected=False
Mandatory=False
NoWarning=False
Date=2018.09.09
History001=ChrisR Initial XPE Plugin Created 2018.09.09
History002=

[Interface]
pBevel1=pBevel1,1,12,4,6,143,121
Desktop_CheckBox="Desktop Shortcut",1,3,10,10,122,18,False,"__Add Desktop Shortcut"
StartMenu_CheckBox="Start Menu Shortcut",1,3,10,28,122,18,True,"__Add Start Menu Shortcut"
TaskBpin_CheckBox="Pin To TaskBar",1,3,10,46,122,18,True,"__Add Pin To Taskbar Shortcut"
StartMpin_CheckBox="Pin To StartMenu",1,3,10,64,122,18,False,"__Add Pin To StartMenu Shortcut"
StartMenuFolder_TextBox="Start Menu Folder:",1,0,10,100,131,18,,"__Start Menu Folder Name"
pImage1=SlideToShutdownScreen.jpg,1,5,156,6,215,121

[Variables]
%ProgramTitle%=SlideToShutDown
%ProgramEXE%=SlideToShutDown.exe
%ProgramFolder%=

[Process]
Echo,"Processing %ScriptTitle%..."
ExtractSectionFiles,%ScriptFile%,AddFiles
// Optional use wallpaper as Slide
If,Not,ExistFile,%GTarget_Win%\Web\Screen\img100.jpg,Begin
If,Not,ExistDir,%GTarget_Win%\Web\Screen,DirMake,%GTarget_Win%\Web\Screen
If,ExistFile,%GTarget_Win%\Web\Wallpaper\Windows\img0.jpg,FileCopy,%GTarget_Win%\Web\Wallpaper\Windows\img0.jpg,%GTarget_Win%\Web\Screen\img100.jpg
End
//--
Run,%ScriptFile%,Add_Shortcuts

[AddFiles]
\Windows\System32\SlideToShutDown.exe

[Add_Shortcuts]
If,%Desktop_CheckBox%,Equal,True,AddShortcut,Desktop,,%SystemRoot%\System32\%ProgramEXE%,,,%WinDir%\System32\shell32.dll|27
If,%StartMenu_CheckBox%,Equal,True,AddShortcut,StartMenu,%StartMenuFolder_TextBox%,%SystemRoot%\System32\%ProgramEXE%,,,%WinDir%\System32\shell32.dll|27
If,%StartMpin_CheckBox%,Equal,True,AddPin,StartMenu
If,%TaskBpin_CheckBox%,Equal,True,AddPin,TaskBar

