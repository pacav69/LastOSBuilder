[Main]
Title=OEM Informations
Type=XPEPlugin
Description=Configure Windows PE OEM Informations.
Author=MaloK
Date=2019.12.27
Credits=
Contact=
Version=1
Level=5
Download_Level=2
Selected=False
Mandatory=False
NoWarning=False
History001=MaloK - 2019.12.27 Initial Plugin Created

[Interface]
pBevel1=pBevel1,1,12,0,3,215,244
Man_TBox=Manufacturer,1,0,6,27,200,21,
Hrs_TBox="Support Hours",1,0,6,67,200,21,
Pho_TBox="Support Phone",1,0,6,107,200,21,
Url_TBox="Support URL",1,0,6,147,200,21,
Logo_Btn="Put Logo File Here*",1,8,5,179,203,25,Provide_Logo,0,True,_Provide_Logo_,True
Info_Lbl="* Should be a BMP file with the dimension of 120×120 Named OEMLogo.bmp",1,1,10,211,197,33,8,Normal

[Variables]
%ProgramTitle%=OEM Informations
%ProgramFolder%=OEM_Info

[Process]
Echo,"Processing %ScriptTitle%..."
//--
If,Not,ExistFile,%GCustom%\%ProgramFolder%\OEMLogo.bmp,Echo,"OEMLogo.bmp has not been found...",Warn
If,ExistFile,%GCustom%\%ProgramFolder%\OEMLogo.bmp,FileCopy,%GCustom%\%ProgramFolder%\OEMLogo.bmp,%GTarget_Sys%
//--
RegHiveLoad,Tmp_Software,%RegSoftware%
RegWrite,HKLM,0x1,Tmp_Software\Microsoft\Windows\CurrentVersion\OEMInformation,Logo,%SystemRoot%\System32\OEMLogo.bmp
RegWrite,HKLM,0x1,Tmp_Software\Microsoft\Windows\CurrentVersion\OEMInformation,Manufacturer,"%Man_TBox%"
RegWrite,HKLM,0x1,Tmp_Software\Microsoft\Windows\CurrentVersion\OEMInformation,SupportHours,"%Hrs_TBox%"
RegWrite,HKLM,0x1,Tmp_Software\Microsoft\Windows\CurrentVersion\OEMInformation,SupportPhone,"%Pho_TBox%"
RegWrite,HKLM,0x1,Tmp_Software\Microsoft\Windows\CurrentVersion\OEMInformation,SupportURL,"%Url_TBox%"
RegHiveUnLoad,Tmp_Software
//--

[Provide_Logo]
If,Not,ExistDir,%GCustom%\%ProgramFolder%,DirMake,%GCustom%\%ProgramFolder%
OpenDir,%GCustom%\%ProgramFolder%
//--

